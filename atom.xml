\begin{align}
&amp;amp;(x+y+z)^3-x^3-y^3-z^3 =k(x+y)(y+z)(z+x) \\\\
&amp;amp;代入x=y=z=1,得24=8k,k=3 \\\\
&amp;amp;原式=3(x+y)(y+z)(z+x)
\end{align}

